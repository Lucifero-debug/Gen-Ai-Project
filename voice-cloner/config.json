{
  "model": "xtts",
  "model_version": "2.0.2",
  
  "audio": {
    "fft_size": 1024,
    "hop_length": 256,
    "win_length": 1024,
    "sample_rate": 22050,
    "mel_fmin": 0,
    "mel_fmax": 8000,
    "num_mels": 80,
    "do_trim_silence": true,
    "trim_db": 45,
    "power": 1.5,
    "preemphasis": 0.97,
    "ref_level_db": 20,
    "do_sound_norm": false,
    "log_func": "np.log10",
    "do_rms_norm": false,
    "db_level": null
  },
  
  "model_args": {
    "gpt_batch_size": 1,
    "enable_redaction": false,
    "kv_cache": true,
    "gpt_checkpoint": null,
    "clvp_checkpoint": null,
    "decoder_checkpoint": null,
    "num_chars": 255,
    "tokenizer_file": "",
    "gpt_max_audio_tokens": 605,
    "gpt_max_text_tokens": 402,
    "gpt_max_prompt_tokens": 70,
    "gpt_layers": 30,
    "gpt_n_model_channels": 1024,
    "gpt_n_heads": 16,
    "gpt_number_text_tokens": 6681,
    "gpt_start_text_token": 6680,
    "gpt_stop_text_token": 0,
    "gpt_num_audio_tokens": 1026,
    "gpt_start_audio_token": 1024,
    "gpt_stop_audio_token": 1025,
    "gpt_code_stride_len": 1024,
    "gpt_use_masking_gt_prompt_approach": true,
    "gpt_use_perceiver_resampler": true,
    "input_sample_rate": 22050,
    "output_sample_rate": 24000,
    "output_hop_length": 256,
    "decoder_output_dim": 1024,
    "d_vector_dim": 512,
    "cond_d_vector_in_each_upsampling_layer": true,
    "duration_const": 102400
  },
  
  "datasets": [
    {
      "formatter": "ljspeech",
      "name": "my_dataset",
      "path": "/content/drive/MyDrive/my_dataset",
      "meta_file_train": "metadata_train.csv",
      "meta_file_val": "metadata_val.csv",
      "language": "en",
      "dataset_type": "tts"
    }
  ],
  
  "text": {
    "text_cleaner": "multilingual_cleaners",
    "language": "en",
    "add_blank": true,
    "normalize_text": true,
    "characters": {
      "characters_class": "TTS.tts.utils.text.characters.IPAPhonemes",
      "vocab": " !\"#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\\]^_`abcdefghijklmnopqrstuvwxyz{|}~",
      "pad": "_",
      "eos": "&",
      "bos": "*",
      "blank": "#",
      "characters": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz!'(),-.:;? ",
      "punctuations": "!'(),-.:;? ",
      "phonemes": "ɑɐɒæɓʙβɔɕçɗɖðʤəɘɚɛɜɝɞɟʄɡɠɢʛɦɧħɥʜɨɪʝɭɬɫɮʟɱɯɰŋɳɲɴøɵɸθœɶʘɹɺɾɻʀʁɽʂʃʈʧʉʊʋⱱʌɣɤʍχʎʏʑʐʒʔʡʕʢǀǁǂǃˈˌːˑʼʴʰʱʲʷˠˤ˞↓↑→↗↘'̩'ᵻ"
    }
  },
  
  "training": {
    "batch_size": 2,
    "eval_batch_size": 1,
    "num_loader_workers": 4,
    "num_eval_loader_workers": 4,
    "run_eval": true,
    "test_delay_epochs": -1,
    "epochs": 1000,
    "text_split_on_newline": true,
    "print_step": 25,
    "plot_step": 100,
    "model_param_stats": false,
    "wandb_project": "xtts_finetuning",
    "log_model_step": 1000,
    "save_step": 1000,
    "save_n_checkpoints": 5,
    "save_checkpoints": true,
    "save_all_best": false,
    "save_best_after": 10000,
    "target_loss": "loss",
    "print_eval": false,
    "test_sentences": [
      "This is a test sentence for evaluation.",
      "The quick brown fox jumps over the lazy dog.",
      "Testing the fine-tuned model with different text samples.",
      "please meet tommorow for a coffee"
    ]
  },
  
  "optimizer": {
    "optimizer": "AdamW",
    "optimizer_params": {
      "betas": [0.9, 0.96],
      "eps": 1e-8,
      "weight_decay": 1e-2
    }
  },
  
  "lr_scheduler": {
    "lr_scheduler": "MultiStepLR",
    "lr_scheduler_params": {
      "milestones": [50000, 150000, 300000],
      "gamma": 0.5,
      "last_epoch": -1
    }
  },
  
  "model_params": {
    "temperature": 0.85,
    "length_penalty": 1.0,
    "repetition_penalty": 2.0,
    "top_k": 50,
    "top_p": 0.85,
    "typical_p": 0.95,
    "decoder_iterations": 30,
    "cond_free_k": 2,
    "diffusion_iterations": 30,
    "cond_free": true,
    "diffusion_temperature": 1.0,
    "speed": 1.0
  },
  
  "fine_tuning": {
    "lr": 5e-6,
    "warmup_steps": 0,
    "max_training_steps": 2000,
    "batch_group_size": 0,
    "loss_masking": true,
    "min_audio_len": 1,
    "max_audio_len": 14,
    "min_text_len": 1,
    "max_text_len": 200,
    "compute_f0": false,
    "f0_cache_path": null,
    "return_wav": false,
    "vocoder": null,
    "reinit_layers": [],
    "freeze_layers": []
  },
  
  "model_dir": null,
  "project_name": "xtts_finetuning",
  "run_name": "xtts_custom_voice",
  "run_description": "XTTS fine-tuning for custom voice cloning",
  "logger_uri": null,
  "tb_logger": true,
  "wandb_logger": false,
  "dashboard_logger": "tensorboard",
  "use_original_sampling_rate": false,
  
  "preprocess_params": {
    "audio_processor": "audio_processor",
    "compute_linear_spec": false,
    "compute_mel_spec": true,
    "mel_spec_cache": true,
    "build_cache": true,
    "cache_path": null,
    "do_feature_loss": false,
    "resample": false,
    "feature_loss_cache": true,
    "normalize_audio": false,
    "audio_normalization": "rms",
    "clip_grad_norm": 1000.0,
    "grad_accum_steps": 1,
    "mixed_precision": false,
    "precision": "fp16",
    "epochs": 1000,
    "batch_size": 2,
    "eval_batch_size": 1,
    "run_eval": true,
    "test_delay_epochs": -1,
    "print_step": 25,
    "tb_model_param_stats": false,
    "save_step": 1000,
    "checkpoint": true,
    "keep_all_best": false,
    "keep_after": 10000,
    "num_loader_workers": 4,
    "num_eval_loader_workers": 4,
    "use_noise_augment": false,
    "output_path": "./output/",
    "distributed_backend": "nccl",
    "distributed_url": "tcp://localhost:54321"
  },
  
  "test_sentences": [
    "This is a test sentence to evaluate the fine-tuned model.",
    "The quality of the generated speech should be clear and natural.",
    "Fine-tuning XTTS allows for custom voice cloning with minimal data."
  ],
  
  "phoneme_cache_path": null,
  "use_phonemes": false,
  "phoneme_language": "en-us",
  "compute_input_seq_cache": true,
  "add_blank": true,
  "enable_eos_bos_chars": false,
  "test_sentences_file": null,
  "phoneme_cache_path": null,
  "speakers_file": null,
  "speaker_id_mapping": {},
  "use_speaker_weighted_sampler": false,
  "speaker_weighted_sampler_alpha": 1.0,
  "use_language_weighted_sampler": false,
  "language_weighted_sampler_alpha": 1.0,
  "use_length_weighted_sampler": false,
  "length_weighted_sampler_alpha": 1.0,
  
  "output_path": "./output/",
  "mixed_precision": false,
  "precision": "fp16",
  "max_text_len": 325,
  "max_audio_len": 255995,
  "min_text_len": 1,
  "min_audio_len": 1,
  "test_delay_epochs": -1,
  "print_step": 25,
  "tb_model_param_stats": false,
  "save_step": 1000,
  "checkpoint": true,
  "keep_all_best": false,
  "keep_after": 10000,
  "num_loader_workers": 4,
  "num_eval_loader_workers": 4,
  "use_noise_augment": false,
  "distributed_backend": "nccl",
  "distributed_url": "tcp://localhost:54321"
}